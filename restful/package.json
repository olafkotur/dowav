{
  "name": "restful",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.ts",
  "scripts": {
    "tsc": "tsc",
    "start": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "build": "tsc && node ./build/index.js",
    "mongo:start": "mongod --dbpath db --fork --logpath /dev/null",
    "mongo:stop": "mongo admin --eval 'db.shutdownServer()' > /dev/null"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "express": "^4.17.1",
    "mongodb": "^3.3.5",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
